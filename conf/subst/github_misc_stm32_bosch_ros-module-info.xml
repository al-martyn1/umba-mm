<umba-mm-module-info>
    <actions>
        <action name="after-checkout">
        open https://yandex.ru/search/?text=$(UmbaMmModuleName)%20after-checkout
        </action>
        <action name="before-remove">
        open https://yandex.ru/search/?text=$(UmbaMmModuleName)%20before-remove
        </action>
    </actions>

    <file match-var="Toolchain" match-to="keil/armclang">
      <file-path>udp_logger/log.c</file-path>
    </file>
    <file match-var="Toolchain" match-to="eclipse/gcc-arm" match-mask="true">
      <file-path>src/apps/nodes/ultrasonic_sensor/ultrasonic_sensor.cpp</file-path>
    </file>
    <file>
      <file-path>src/apps/nodes/ultrasonic_sensor/ultrasonic_sensor.h</file-path>
    </file>
